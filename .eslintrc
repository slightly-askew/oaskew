{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["react-hooks"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
